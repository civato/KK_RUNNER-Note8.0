#5100


export ARCH=arm
export CROSS_COMPILE=~/KERNEL/TOOLCHAINS/arm-2010q1/bin/arm-none-linux-gnueabi-
make 5100_kk-runner_01_defconfig
make -j5
rm -rf mod
mkdir mod
cp `find ./ | grep .ko$` modules.order mod/



