case $1 in
cpu)
  echo `cat /sys/devices/system/cpu/cpu0/cpufreq/UV_mV_table`
  ;;
cpuinfo)
  echo `cat /proc/cpuinfo`
  ;;
modules)
  echo `cat /proc/modules`
  ;;
kernel)
  echo `cat /proc/version`
  ;;
temp)
  echo Battery: $(($(cat /sys/class/power_supply/battery/temp) / 10))°C "|" Status: `cat /sys/class/power_supply/battery/status` "|" Health: `cat /sys/class/power_supply/battery/health`
  ;;
freq)
  echo CPU: $(($(cat /sys/devices/platform/sec-thermistor/temperature) / 10))°C "|" CPU Freq: $(($(cat /sys/devices/system/cpu/cpu0/cpufreq/scaling_cur_freq) / 1000))MHz
  ;;
offline)
  echo ONLINE CPU0: `cat /sys/devices/system/cpu/cpu0/online` "|" CPU1: `cat /sys/devices/system/cpu/cpu1/online` "|" CPU2: `cat /sys/devices/system/cpu/cpu2/online` "|" CPU3: `cat /sys/devices/system/cpu/cpu3/online`
  ;;
entropy)
    echo "`cat /proc/sys/kernel/random/entropy_avail`";
  ;;
dyna)
  echo `cat /sys/kernel/dyn_fsync/Dyn_fsync_version`
  ;;
gpu)
  echo GPU Freq: $(cat /sys/module/mali/parameters/mali_gpu_clk)MHz
  ;;
gpuinfo)
  echo `cat /sys/class/misc/gpu_control/gpu_clock_control`
  ;;
esac
